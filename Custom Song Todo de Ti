use_bpm 128
use_synth :bass_highend
live_loop :square do
  play :bb4
  sleep 0.5
  play :eb4
  sleep 0.5
  play :gb4
  sleep 0.5
  play :eb4
  sleep 0.5
  play :ab4
  sleep 0.5
  play :db4
  sleep 0.5
  play :f4
  sleep 0.5
  play :db4
  sleep 0.5
  play :f4
end
sleep 4
live_loop :blip do
  sample :elec_blip2,amp: 3,beat_stretch: 4
  sleep 1
end
sleep 8
live_loop :chip do
  sample :bd_chip
  sleep 2
end
